<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.data.dormitory.mbg.mapper.StuMapper">

    <!--根据房号、床号、楼号获取学生信息-->
    <select id="getStuByNum" resultType="com.data.dormitory.mbg.model.Stu">
        SELECT *
        FROM stu s,
             bed_sid b
        WHERE b.bbid = #{bbid}
          AND b.bid = #{bid}
          AND b.did = #{did}
          AND s.sid = b.sid

    </select>

</mapper>
